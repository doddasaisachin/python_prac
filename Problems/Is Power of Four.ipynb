    "import math\n",
    "\n",
    "def isPowerOfFour( n: int) -> bool:\n",
    "    if n<=0:\n",
    "        return False\n",
    "    rem=math.floor(math.log(n,4))\n",
    "    return 4**rem==n"
   ]
