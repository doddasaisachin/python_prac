   "source": [
    "Given the head of a singly linked list, reverse the list, and return the reversed list."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "90f4c3d0",
   "metadata": {},
   "outputs": [],
   "source": [
    "def reverseList(head):\n",
    "    temp=head\n",
    "    p=head\n",
    "    q=r=None\n",
    "    while p is not None:\n",
    "        r=q\n",
    "        q=p\n",
    "        p=p.next\n",
    "        q.next=r\n",
    "    return q"
   ]
  },
