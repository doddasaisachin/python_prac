   "source": [
    "def function(array):\n",
    "    c=array.count(0)\n",
    "    idx=0\n",
    "    while idx<len(array):\n",
    "        if array[idx]==0:\n",
    "            array.pop(idx)\n",
    "        else:\n",
    "            idx+=1\n",
    "    for i in range(c):\n",
    "        array.append(0)\n",
    "    return array"
   ]
